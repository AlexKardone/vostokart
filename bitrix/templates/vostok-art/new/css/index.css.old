@media (max-width: 767px) {
  .menu-wrap .header-line .logo-wrap .logo {
    background: url(../img/logo_footer.png) no-repeat center;
    background-size: contain;
    width: 40px;
    height: 40px;
  }
  #sidenav {
    display: none;
  }
  .page-content .content-wrap.catalog-hack ul li {
    flex: 0 0 50%;
  }
}
.add-to-card-popup {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.9);
  display: none;
  z-index: 999999999;
}
.add-to-card-popup.show {
  display: table;
}
.add-to-card-popup .window-wrap {
  display: table-cell;
  vertical-align: middle;
  pointer-events: none;
}
.add-to-card-popup .window {
  position: relative;
  pointer-events: auto;
  margin: 0 auto;
  max-width: 500px;
  overflow: auto;
  background-color: #fff;
  padding: 20px;
  color: #000;
  max-height: 100%;
}
.add-to-card-popup .window .close-bt {
  display: block;
  position: absolute;
  right: 15px;
  top: 0px;
  font-size: 20px;
  transform: scale(1.5, 1);
  font-weight: 300;
  padding: 5px;
}
.add-to-card-popup .window a[data-card-action='remove'] {
  color: #656565;
  border-bottom: 1px dashed #656565;
  display: table;
}
.add-to-card-popup .window a[data-card-action='remove']:hover {
  color: #000;
  border-bottom: 1px dashed #000;
}
.add-to-card-popup .window h2 {
  font-size: 20pt;
  color: #aa7c2b;
}
.add-to-card-popup .window .scroll {
  max-height: 200px;
  overflow-y: auto;
  border: 1px solid #e6e6e6;
  padding: 5px;
}
.add-to-card-popup .window .title {
  font-weight: 500;
}
.add-to-card-popup .window ul {
  margin: 0;
  padding: 0;
  list-style: none;
}
.add-to-card-popup .window ul li {
  display: flex;
  padding: 0;
  margin-bottom: 10px;
}
.add-to-card-popup .window ul li img {
  width: 50px;
  height: 50px;
  margin-right: 10px;
  object-fit: cover;
}
.add-to-card-popup .window .button {
  padding: 10px 15px;
  background-color: #aa7c2b;
  display: inline-block;
  color: #fff;
  margin-right: 10px;
  margin-bottom: 10px;
  border: 1px solid transparent;
}
.add-to-card-popup .window .button:hover {
  background-color: #f29315;
}
.add-to-card-popup .window .button.place-next {
  border: 1px solid #aa7c2b;
  background: transparent;
  color: #aa7c2b;
}
.add-to-card-popup .window .button.place-next:hover {
  border: 1px solid #f29315;
  background: transparent;
  color: #f29315;
}
.add-to-card-popup .window p {
  font-size: 10pt;
  color: #333;
}
.fancybox-skin h2 {
  font-size: 20pt;
  color: #aa7c2b;
}
@media (max-width: 550px) {
  .add-to-card-popup .window .button {
    display: block;
    text-align: center;
  }
  .menu-wrap .main-menu .search-bar {
    display: none;
  }
}
@media (max-width: 500px) {
  .add-to-card-popup .window {
    min-height: 100%;
  }
}
