body {
  background-color: #171618;
  color: #fff;
}
a {
  color: #aa7c2b;
}
a:hover {
  color: #f29315;
  text-decoration: none;
}
.mobile-menu {
  display: none;
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.9);
  z-index: 999999999;
}
.mobile-menu .close-area {
  width: calc(100% - 250px);
  min-width: 10%;
  height: 100%;
  position: absolute;
  top: 0;
  right: 0;
}
.mobile-menu .mobile-menu-wrap {
  width: 250px;
  max-width: 90%;
  background-color: #151417;
  height: 100%;
  overflow: auto;
}
.mobile-menu .mobile-menu-wrap .search-bar {
  width: 100%;
  padding: 0 20px;
}
.mobile-menu .mobile-menu-wrap .search-bar input[type=text] {
  color: #808080;
  border: 1px solid #39393c;
  background: #1f1f22;
  outline: none;
  height: 30px;
}
.mobile-menu .mobile-menu-wrap .search-bar input[type=submit] {
  color: #fff;
  background: #1f1f23;
  border: 1px solid #39393c;
  line-height: 21px;
  cursor: pointer;
  height: 30px;
}
.mobile-menu .mobile-menu-wrap ul {
  list-style: none;
  margin: 0;
  padding: 20px 0;
}
.mobile-menu .mobile-menu-wrap ul li {
  width: 100%;
}
.mobile-menu .mobile-menu-wrap ul li a {
  display: block;
  width: 100%;
  padding: 10px 25px;
  color: #909090;
}
.mobile-menu .mobile-menu-wrap ul li.active a {
  color: #ec6d1c;
}
.mobile-menu.show {
  display: block;
}
.menu-wrap {
  height: 109px;
}
.menu-wrap .space-between-block {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.menu-wrap .header-line {
  background-color: #000000;
  height: 55px;
  width: 100%;
}
.menu-wrap .header-line .container {
  height: 100%;
}
.menu-wrap .header-line .phone-wrap {
  font-size: 20px;
  font-weight: 500;
}
.menu-wrap .header-line .logo-wrap .logo {
  background: url(../img/logo.png) no-repeat center;
  background-size: contain;
  width: 370px;
  height: 55px;
  margin-left: -10px;
}
.menu-wrap .main-menu {
  background-color: #000000;
}
.menu-wrap .main-menu.fixed {
  width: 100%;
  position: fixed;
  z-index: 999;
  top: 0;
}
.menu-wrap .main-menu .close-bt {
  display: none;
  position: absolute;
  right: 10px;
  z-index: 9999999999;
}
.menu-wrap .main-menu .mobile-menu-bt {
  display: none;
  font-size: 22pt;
  transform: scale(1, 0.8);
  cursor: pointer;
}
.menu-wrap .main-menu .container {
  padding: 0 15px 0 5px;
}
.menu-wrap .main-menu ul.menu {
  list-style: none;
  padding: 0;
  margin: 0;
}
.menu-wrap .main-menu ul.menu li {
  display: inline-flex;
}
.menu-wrap .main-menu ul.menu li a {
  padding: 15px 10px;
  color: #fff;
}
.menu-wrap .main-menu ul.menu li a:hover {
  color: #f29315;
}
.menu-wrap .main-menu ul.menu li.active {
  position: relative;
}
.menu-wrap .main-menu ul.menu li.active a {
  color: #ec6d1c;
  padding-bottom: 7px;
  position: relative;
}
.menu-wrap .main-menu ul.menu li.active a:after {
  content: '';
  display: block;
  position: absolute;
  height: 3px;
  background-color: #ec6d1c;
  left: 10px;
  right: 10px;
  bottom: 0;
}
.menu-wrap .main-menu a.card-button {
  display: block;
  position: relative;
  height: 100%;
  background: #000;
  padding: 14px 5px;
  padding-left: 35px;
  color: #f7af82;
}
.menu-wrap .main-menu a.card-button:before {
  content: '';
  display: inline-block;
  background: url(../img/card.svg) no-repeat center;
  width: 20px;
  height: 20px;
  top: 16px;
  left: 5px;
  position: absolute;
}
.menu-wrap .main-menu .search-bar input[type=text] {
  color: #808080;
  border: 1px solid #39393c;
  background: #1f1f22;
  padding: 0 5px;
  outline: none;
}
.menu-wrap .main-menu .search-bar input[type=submit] {
  color: #fff;
  background: #1f1f23;
  border: 1px solid #39393c;
  line-height: 21px;
  cursor: pointer;
}
.page-content > .row {
  display: block;
  margin: 0;
}
.page-content img {
  max-width: 100%;
}
.page-content .breadcrumb-block-wrap .bx-breadcrumb {
  max-width: 100%;
}
.page-content .breadcrumb-block-wrap .bx-breadcrumb .bx-breadcrumb-item {
  display: inline;
  float: none;
  white-space: inherit;
}
.page-content .breadcrumb-block-wrap .bx-breadcrumb i {
  margin-right: 10px;
  height: auto;
  width: auto;
  font-size: 25px;
  color: #696969;
}
.page-content .catalog-item h1 {
  font-size: 20pt;
  color: #aa7c2b;
}
.page-content .catalog-item .info {
  color: #ccc;
}
.page-content .catalog-item .buy-block {
  background: #0f0f0f;
  padding: 10px;
}
.page-content .catalog-item .buy-block .price {
  font-size: 20px;
  font-weight: 700;
}
.page-content .catalog-item .buy-block .price s {
  font-size: 12px;
  color: #f29315;
  font-weight: 500;
}
.page-content .catalog-item .buy-block .discount {
  font-size: 20px;
  font-weight: 700;
}
.page-content .catalog-item .buy-block .remainder {
  font-size: 12px;
  margin-top: 10px;
}
.page-content .catalog-item .buy-block .percents {
  padding: 3px 5px;
  background: #ec6d1c;
  display: inline-block;
  margin-left: 10px;
  font-weight: 700;
  font-size: 14px;
}
.page-content .catalog-item .buy-block .place-order {
  display: inline-block;
  margin-bottom: 0;
  color: #fff !important;
  font-size: 0.938rem;
  background: #ec6d1c;
  padding: 10px;
}
.page-content .catalog-item .buy-block .place-order:hover {
  background: #f29315;
}
.page-content .content-wrap {
  background-color: #232226;
  padding: 20px;
  margin-bottom: 20px;
}
.page-content .content-wrap h1,
.page-content .content-wrap h2.title {
  margin: 0 0 15px 0;
  padding: 0 0 15px 0;
  border-bottom: 1px solid #2f2e30;
  font-size: 18px;
  text-transform: uppercase;
  color: #b3b3b3;
}
.page-content .content-wrap .cat-item-wrap {
  margin-bottom: 15px;
  height: calc(100% - 15px);
}
.page-content .content-wrap .cat-item-wrap > a {
  display: block;
  width: 100%;
  height: 100%;
  padding-bottom: 10px;
  border-bottom: 5px solid transparent;
}
.page-content .content-wrap .cat-item-wrap > a:hover {
  border-bottom: 5px solid #ec6d1c;
}
.page-content .content-wrap .cat-item-wrap .image-wrap {
  margin: 0 0 5px 0;
  padding: 0 0 15px 0;
  border-bottom: 1px solid #2f2e30;
}
.page-content .content-wrap .cat-item-wrap .image-wrap img {
  width: 100%;
  height: auto;
}
.page-content .content-wrap .cat-item-wrap .title {
  text-align: center;
  font-weight: 500;
  font-size: 10pt;
}
.page-content .content-wrap .cat-item-wrap .article {
  color: #ccc;
  font-size: 10pt;
  text-align: center;
  margin-top: 10px;
}
.page-content .content-wrap .text-block {
  text-align: center;
}
.page-content .content-wrap .text-block .social {
  display: inline-flex;
}
.page-content .content-wrap .text-block .social i {
  margin-right: 5px;
}
.page-content .content-wrap.catalog-hack ul {
  list-style: none;
  display: flex;
  padding: 0;
  margin: 0;
  flex-wrap: wrap;
  margin-right: -15px;
  margin-left: -15px;
}
.page-content .content-wrap.catalog-hack ul li {
  display: block;
  flex: 0 0 33.333333%;
  position: relative;
  width: 100%;
  min-height: 1px;
  padding-right: 15px;
  padding-left: 15px;
  margin-bottom: 20px;
  overflow: hidden;
}
.page-content .content-wrap.catalog-hack ul li a {
  background-color: #171618;
  display: block;
  height: 100%;
  border-bottom: 5px solid transparent;
}
.page-content .content-wrap.catalog-hack ul li a:hover {
  border-bottom: 5px solid #ec6d1c;
}
.page-content .content-wrap.catalog-hack ul li img {
  width: 100%;
}
.page-content .content-wrap.catalog-hack ul li .info {
  display: block;
  text-align: center;
  font-weight: 500;
  font-size: 10pt;
  width: 100%;
  color: #b3b3b3;
  padding: 15px 10px;
}
.page-content .content-wrap.catalog-hack ul li .info .price,
.page-content .content-wrap.catalog-hack ul li .info .discount {
  color: #f29315;
}
.page-content .content-wrap.catalog-hack ul li .info .price_old {
  font-size: 12px;
  color: #aa7c2b;
  font-weight: 500;
}
.page-content .content-wrap.pages {
  color: #ccc;
}
.page-content .content-wrap.pages h1 {
  color: #aa7c2b;
}
i {
  display: block;
  width: 32px;
  height: 32px;
}
i.fb {
  background: url(../img/fb.png) no-repeat center;
}
i.vk {
  background: url(../img/vk.png) no-repeat center;
}
i.insta {
  background: url(../img/insta.png) no-repeat center;
}
footer {
  padding: 30px 0;
  background-color: #232226;
  border-top: 2px solid #0e0e0e;
}
footer .copy {
  font-size: 8pt;
  color: #aaa;
}
footer .logo-footer-wrap {
  width: 100%;
  height: 100%;
  background: url(../img/logo_footer.png) no-repeat right center;
}
#sidenav {
  margin: 0;
  padding: 0;
  list-style: none;
  background-color: #232226;
  font-size: 10pt;
}
#sidenav > li {
  background-color: #0F100F;
}
#sidenav > li a {
  padding: 10px 15px;
  display: block;
  position: relative;
  color: #909090;
}
#sidenav > li a:hover {
  color: #fff;
}
#sidenav > li a .title {
  display: inline-block;
  width: calc(100% - 45px);
}
#sidenav > li a .num {
  background-color: #302f33;
  border-radius: 10px;
  display: inline-block;
  padding: 0 5px;
  position: absolute;
  text-align: right;
  right: 15px;
  top: 10px;
}
#sidenav > li ul {
  margin: 0;
  padding: 0;
  list-style: none;
}
#sidenav > li ul > li {
  background-color: #232226;
}
#sidenav > li ul > li.active {
  border-left: 1px solid #ec6d1c;
}
#sidenav > li.active {
  border-left: 1px solid #ec6d1c;
}
#sidenav > li.active ul {
  display: block;
}
.bx-yandex-map,
.ymaps-map {
  max-width: 100%;
}
[name=SIMPLE_FORM_1] h1 {
  color: #aa7c2b;
}
[name=SIMPLE_FORM_1] input[type=text] {
  width: 100%;
  margin-bottom: 10px;
  padding: 5px 10px;
}
[name=SIMPLE_FORM_1] input[type=submit] {
  border-style: solid;
  border-width: 0px;
  cursor: pointer;
  font-family: "Helvetica Neue", "Helvetica", Helvetica, Arial, sans-serif;
  font-weight: normal;
  line-height: normal;
  margin: 0 0 1.25rem;
  position: relative;
  text-decoration: none;
  text-align: center;
  -webkit-appearance: none;
  -webkit-border-radius: 0;
  display: inline-block;
  padding-top: 1rem;
  padding-right: 2rem;
  padding-bottom: 1.0625rem;
  padding-left: 2rem;
  font-size: 1rem;
  background-color: #aa7c2b;
  border-color: #886322;
  color: white;
  transition: background-color 300ms ease-out;
}
[name=SIMPLE_FORM_1] > div > div:last-of-type {
  display: none;
}
@media (max-width: 991px) {
  .menu-wrap .menu-line .container {
    padding: 0 15px 0 15px;
  }
  .menu-wrap .menu-line .mobile-menu-bt {
    display: inline-block;
  }
  .menu-wrap .menu-line ul.menu {
    display: none;
  }
  .menu-wrap .header-line .logo-wrap .logo {
    margin-left: 0;
  }
